# ============================================
# EasyScale SDR Closer - n8n Workflows Config
# ============================================

# --- Evolution API ---
EVOLUTION_API_URL=https://evo.example.com
EVOLUTION_API_KEY=your_evolution_api_key
EVOLUTION_INSTANCE_NAME=your_instance_name

# --- FastAPI (Agentes SDR) ---
FASTAPI_BASE_URL=https://ade.easyscale.co
FASTAPI_API_KEY=your_fastapi_api_key

# --- Supabase (PostgreSQL) ---
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
SUPABASE_DB_HOST=db.your-project.supabase.co
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=postgres
SUPABASE_DB_USER=postgres
SUPABASE_DB_PASSWORD=your_db_password

# --- n8n ---
N8N_WEBHOOK_BASE_URL=https://n8n.easyscale.co/webhook
N8N_API_KEY=your_n8n_api_key

# --- GitOps ---
GITHUB_WEBHOOK_SECRET=your_github_webhook_secret
